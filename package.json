{
  "name": "logistics-route-optimizer",
  "version": "1.0.0",
  "description": "Logistics Route Optimizer DApp with FHEVM confidential computation",
  "main": "index.html",
  "scripts": {
    "dev": "npx http-server . -p 3000 -c-1 --cors",
    "start": "npx http-server . -p 8080 -c-1 --cors",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:sepolia": "hardhat test test/LogisticsRouteOptimizer.sepolia.test.ts --network sepolia",
    "test:coverage": "hardhat coverage",
    "deploy": "node scripts/deploy.js",
    "deploy:local": "hardhat run scripts/deploy.js --network hardhat",
    "deploy:sepolia": "hardhat run scripts/deploy.js --network sepolia",
    "deploy:simple": "hardhat run scripts/deploy-simple.ts --network sepolia",
    "verify": "node scripts/verify.js",
    "verify:sepolia": "hardhat run scripts/verify.js --network sepolia",
    "interact": "node scripts/interact.js",
    "interact:sepolia": "hardhat run scripts/interact.js --network sepolia",
    "simulate": "node scripts/simulate.js",
    "simulate:sepolia": "hardhat run scripts/simulate.js --network sepolia",
    "size": "hardhat size-contracts",
    "typechain": "hardhat typechain",
    "clean": "hardhat clean",
    "node": "hardhat node",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "lint:ts": "eslint . --ext .ts,.tsx,.js,.jsx --ignore-path .gitignore",
    "lint": "npm run lint:sol && npm run lint:ts",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,sol,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,sol,json,md}\"",
    "security:check": "npm run lint:sol && npm run size",
    "prepare": "husky install"
  },
  "keywords": [
    "logistics",
    "route-optimization",
    "fhevm",
    "blockchain",
    "ethereum",
    "dapp",
    "fhe",
    "privacy"
  ],
  "author": "Logistics Route Optimizer",
  "license": "MIT",
  "dependencies": {
    "@fhevm/solidity": "^0.8.0",
    "fhevmjs": "^0.6.0"
  },
  "devDependencies": {
    "@fhevm/hardhat-plugin": "^0.1.0",
    "@fhevm/mock-utils": "^0.1.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-ignition": "^0.15.13",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.14",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@nomicfoundation/ignition-core": "^0.15.13",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@zama-fhe/oracle-solidity": "^0.1.0",
    "@zama-fhe/relayer-sdk": "^0.2.0",
    "chai": "^4.3.10",
    "dotenv": "^16.3.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "ethers": "^6.9.0",
    "hardhat": "^2.19.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.12.0",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-solidity": "^1.3.0",
    "solhint": "^4.0.0",
    "solidity-coverage": "^0.8.5",
    "ts-node": "^10.9.0",
    "typechain": "^8.3.0",
    "typescript": "^5.3.0"
  }
}
